{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadOnlyAccess",
            "Effect": "Allow",
            "Action": [
                 "cloudtrail:LookupEvents",
                 "cloudtrail:GetTrailStatus",
                 "config:Describe*",
                 "config:Get*"
            ],
            "Resource": [
        "arn:aws:cloudtrail:*:${AccountId}:trail/*",
        "arn:aws:config:*:${AccountId}:config-rule/*"
      ]
            
        },
        {
            "Sid": "DenySensitiveActions",
            "Effect": "Deny",
            "Action": [
                "iam:CreateUser",
                "iam:DeleteUser",
                "iam:CreateAccessKey",
                "s3:DeleteObject",
                "ec2:TerminateInstances",
                "rds:DeleteDBInstance"
            ],
            "Resource": "*"
        }
    ]
}


